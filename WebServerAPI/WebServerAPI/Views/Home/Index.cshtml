                                            <input id="year-column-thu-tuc-cb" title="Chọn thời gian" style="width: 100%" />
                                        </div>
                                    </div>
                                </div>
                                <div id="table-core-thu-tuc-cb">
                                    <div id="image-thu-tuc-cb" style="text-align: center"><input type="image" @*src="~/user.png"*@ alt="Hình ảnh cán bộ" /></div>
                                    <div id="info-thu-tuc-left" style="float:left; width: 32%">
                                        <div>Mã cán bộ: </div>
                                        <div>Họ tên: </div>
                                        <div>Mã bộ phận: </div>
                                        <div>Tên bộ phận: </div>
                                    </div>
                                    <div id="info-thu-tuc-right" style="float:right; width: 68%">
                                        <div id="id-thu-tuc-cb"><span></span></div>
                                        <div id="name-thu-tuc-cb"><span></span></div>
                                        <div id="id-thu-tuc-bp"><span></span></div>
                                        <div id="name-thu-tuc-bp"><span></span></div>
                                    </div>
                                </div>
